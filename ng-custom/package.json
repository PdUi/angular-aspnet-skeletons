  "name": "ng-custom",
  "version": "1.0.0",
  "scripts": {
    "lint": "tslint -c config/tslint.json src/**/*.ts",
    "prebuild": "node build/clean.js && gulp --gulpfile ./src/styles/semantic/gulpfile.js build",
    "build:dev": "npm run prebuild && webpack --config ./config/webpack.dev.js",
    "launch:dev": "npm run build:dev && webpack-dev-server --config ./config/webpack.dev.js",
    "expose:dev": "concurrently \"npm run launch:dev\" \"lt --port 8080 --subdomain peinearydevelopment\"",
    "prebuild:prod": "npm run prebuild && nsp check  && npm run lint && node build/prebuild.prod.js",
    "build:prod": "ngc -p aot/tsconfig.aot.json && rollup -c config/rollup-config.js && node build/postbuild.prod.js",
    "launch:prod": "npm run build:prod && browser-sync start --config config/bs-config.js",
    "expose:prod": "concurrently \"npm run launch:prod\" \"lt --port 8080 --subdomain peinearydevelopment\"",
    "launch:sme:dev": "source-map-explorer dist/vendor.bundle.js",
    "launch:sme:prod": "source-map-explorer aot/dist/build.c15b16c40b12cdabce7bb264c17707d83e69d1aa04f1298be7e55aedc10d5ae4.js aot/dist/build.c15b16c40b12cdabce7bb264c17707d83e69d1aa04f1298be7e55aedc10d5ae4.js.map",
    "lighthouse": "node_modules/.bin/lighthouse http://localhost/ngcustom --output json",
    "test": "jest --config ./config/jest-config.json"
  },
  "dependencies": {
    "@angular/common": "^4.0.3",
    "@angular/compiler": "^4.0.3",
    "@angular/core": "^4.0.3",
