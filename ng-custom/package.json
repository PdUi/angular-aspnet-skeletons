{
  "name": "ng-custom",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "semantic:build": "node_modules/.bin/gulp --gulpfile ./semantic/gulpfile.js build",
    "clean": "rimraf aot && rimraf dist && rimraf semantic/dist",
    "prebuild:dev": "npm run clean && npm run semantic:build",
    "build:dev": "node_modules/.bin/webpack --config ./config/webpack.dev.js",
    "preserve:dev": "npm run build:dev",
    "serve:dev": "node_modules/.bin/webpack-dev-server --config ./config/webpack.dev.js",
    "prebuild:prod": "npm run securityscan  && npm run lint && npm run clean && mkdirp aot/src && ncp src aot/src && ncp config/tsconfig.aot.json aot/tsconfig.aot.json",
    "build:prod": "npm run semantic:build && npm run compile:aot && npm run rollup",
    "postbuild:prod": "node build/postbuild.prod.js",
    "compile:aot": "node_modules/.bin/ngc -p aot/tsconfig.aot.json",
    "rollup": "node_modules/.bin/rollup -c config/rollup-config.js",
    "securityscan": "node_modules/.bin/nsp check",
    "lint": "node_modules/.bin/tslint -c config/tslint.json src/**/*.ts"
  },
  "author": "Peineary Development",
  "license": "MIT",
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.0.3",
    "@types/node": "^7.0.13",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "codelyzer": "^3.0.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "nsp": "^2.6.3",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-hash": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2",
    "semantic-ui": "^2.2.10",
    "style-loader": "^0.16.1",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@angular/common": "^4.0.3",
    "@angular/compiler": "^4.0.3",
    "@angular/core": "^4.0.3",
    "@angular/platform-browser": "^4.0.3",
    "@angular/platform-browser-dynamic": "^4.0.3",
    "@angular/router": "^4.0.3",
    "core-js": "^2.4.1",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.5"
  }
}
