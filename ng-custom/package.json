    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.13",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "browser-sync": "^2.18.8",
    "codelyzer": "^3.0.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
